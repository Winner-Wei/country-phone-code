.wrap_container {
    width: 100%;
    font-size: 28rpx;
    background: #F5F5F5;
    color: #4a4a4a;
}
.to_top {
    position: fixed;
    top: 0;
    z-index: 9999;
    background: #F2F2F2;
}
.every_content {
    width: 100%;
}
.every_content .out_content {
    width: 100%;
    background: #fff;
    padding-right: 80rpx;
}
.title {
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    padding-left: 35rpx;
}
.ind_content {
    display: flex;
    justify-content: space-between;
    align-items: center;
    width: 100%;
    height: 80rpx;
    padding-left: 35rpx;
    padding-right: 86rpx;
    box-sizing: border-box;
    border-bottom: 2rpx solid #F2F2F2;
}
.code_right {
    position: fixed;
    top: 50%;
    right: 10rpx;
    transform: translateY(-50%);
    width: 50rpx;
    text-align: center;
    font-size: 20rpx;
    font-weight: 500;
    color: #6981FF;
}